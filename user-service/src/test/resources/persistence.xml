<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" version="2.0">
    <persistence-unit name="user_database">
        <properties>

            <property name="connection.url" value="jdbc:hsqldb:hsql://localhost:9001"/>
            <property name="connection.driver_class" value="org.hsqldb.jdbcDriver"/>
            <property name="dialect" value="org.hibernate.dialect.HSQLDialect"/>

            <property name="connection.username" value="sa"/>
            
        </properties>
    </persistence-unit>
</persistence>
